			   (%sort-ref "Int" nil))
			  (%sort-ref "Bool" nil)
			  (%opattrs nil nil nil nil nil nil nil t)
			  nil)
			 (%op-decl ("_" "<" "_")
			  ((%sort-ref "Int" nil)
			   (%sort-ref "Int" nil))
			  (%sort-ref "Bool" nil)
			  (%opattrs nil nil nil nil nil nil nil t)
			  nil)
			 (%op-decl ("_" "<=" "_")
			  ((%sort-ref "Int" nil)
			   (%sort-ref "Int" nil))
			  (%sort-ref "Bool" nil)
			  (%opattrs nil nil nil nil nil nil nil t)
			  nil)
			 (%op-decl ("_" ">" "_")
