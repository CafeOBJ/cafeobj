			 (%axiom-decl :equation (":BDEMOD") ("I" "rem" "NJ")
			  ((%slisp (rem i nj))) nil nil)
			 (%axiom-decl :equation (":BDEMOD") ("NJ" "divides" "I")
			  ((%slisp (= 0 (rem i nj)))) nil nil)
			 (%axiom-decl :equation (":BDEMOD") ("I" "<" "J")
			  ((%slisp (< i j))) nil nil)
			 (%axiom-decl :equation (":BDEMOD") ("I" "<=" "J")
			  ((%slisp (<= i j))) nil nil)
			 (%axiom-decl :equation (":BDEMOD") ("I" ">" "J")
			  ((%slisp (> i j))) nil nil)
			 (%axiom-decl :equation (":BDEMOD") ("I" ">=" "J")
			  ((%slisp (>= i j))) nil nil)
			 (%axiom-decl :equation (":BDEMOD") ("s" "I")
			  ("1" "+" "I") nil nil))))
