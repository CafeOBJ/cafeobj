    (eval-ast-if-need '(%provide "INT"))
    (eval-ast-if-need '(%provide "int"))
    ))

(defun chaos-int-tram-interface ()
  (setq *z-int* (get-z-module-or-panic "INT"))
  (push *z-int* *tram-builtin-modules*)
  )

(eval-when (:execute :load-toplevel)
  (chaos-install-int)
  (chaos-int-tram-interface))

