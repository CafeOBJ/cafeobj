(defun chaos-character-tram-interface ()
  (setq *z-char* (get-z-module-or-panic "CHARACTER"))
  (push *z-char* *tram-builtin-modules*)
  )

(eval-when (:execute :load-toplevel)
  (chaos-install-character)
  (chaos-character-tram-interface))

